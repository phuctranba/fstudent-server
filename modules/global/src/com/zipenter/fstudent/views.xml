<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">

    <view entity="fstudent_UserProfile" name="userProfile-view" extends="_local">
        <property name="user" view="_minimal"/>
        <property name="school" view="_minimal"/>
        <property name="major" view="_minimal">
            <property name="faculty"/>
        </property>
        <property name="district" view="_minimal"/>
    </view>
    <view entity="fstudent_Provincial" name="provincial-view" extends="_local"/>
    <view entity="fstudent_School" name="school-view" extends="_local">
        <property name="provincial" view="_minimal">
            <property name="area"/>
        </property>
    </view>
    <view entity="fstudent_Major" name="major-view" extends="_local"/>
    <view entity="fstudent_District" name="district-view" extends="_local">
        <property name="provincial" view="_minimal">
            <property name="area"/>
        </property>
    </view>
    <view entity="fstudent_Book" name="book-view" extends="_local">
        <property name="user" view="_minimal"/>
        <property name="major" view="_minimal"/>
    </view>
    <view entity="fstudent_BookCase" name="bookCase-view" extends="_local">
        <property name="user" view="_minimal"/>
        <property name="book" view="_minimal"/>
    </view>
</views>